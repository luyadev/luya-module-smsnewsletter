{
    "name" : "luyadev/luya-module-smsnewsletter",
    "description" : "SMS Newseltter module for ASPSMS gateway.",
    "type" : "luya-module",
    "keywords" : ["luya"],
    "license": "MIT",
    "authors" : [
        {
            "name": "Basil Suter",
            "email": "basil@nadar.io"
        }
    ],
    "require": {
        "luyadev/luya-module-admin": ">=1.2",
        "nadar/aspsms": "^1.0",
        "giggsey/libphonenumber-for-php": "^8.9"
    },
    "require-dev" : {
        "luyadev/luya-testsuite" : "^2.0"
    },
    "autoload": {
        "psr-4": {
            "luya\\smsnewsletter\\": "src/"
        }
    },
    "config": {
        "fxp-asset": {
            "enabled": false
        },
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "luyadev/luya-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}